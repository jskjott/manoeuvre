{
	"name": "Manoeuvre",
	"version": "0.1.0",
	"description": "Multiple Carets in the browser",
	"manifest_version": 2,

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [{
		"css": ["styles.css"],
		"js": ["vue.min.js", "contentScript.js"],
		"matches": ["<all_urls>"]
	}]
}